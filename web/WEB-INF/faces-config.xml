        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <converter>
        <converter-id>lokalizacjaConverter</converter-id>
        <converter-class>config.LokalizacjaConverter</converter-class>
    </converter>
    
    <managed-bean>
        <managed-bean-name>klientBean</managed-bean-name>
        <managed-bean-class>controllers.KlientBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
     <managed-bean>
        <managed-bean-name>loginBean</managed-bean-name>
        <managed-bean-class>controllers.LoginBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
     <managed-bean>
        <managed-bean-name>seansBean</managed-bean-name>
        <managed-bean-class>controllers.SeanseBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    
     <managed-bean>
        <managed-bean-name>currentKlient</managed-bean-name>
        <managed-bean-class>controllers.CurrentKlient</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
