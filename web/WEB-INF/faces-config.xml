        <managed-bean-name>wybranySeansBean</managed-bean-name>
        <managed-bean-class>controllers.WybranySeansBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>biletBean</managed-bean-name>
        <managed-bean-class>controllers.BiletBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-name>filmBean</managed-bean-name>
        <managed-bean-class>controllers.pracownik.FilmBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
</faces-config>
